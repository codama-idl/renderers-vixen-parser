{
    "name": "@codama/renderers-vixen-parser",
    "version": "1.2.8",
    "description": "Renders Yellowstone Account and Instruction Parsers",
    "exports": {
        "types": "./dist/types/index.d.ts",
        "node": {
            "import": "./dist/index.node.mjs",
            "require": "./dist/index.node.cjs"
        }
    },
    "main": "./dist/index.node.cjs",
    "module": "./dist/index.node.mjs",
    "types": "./dist/types/index.d.ts",
    "type": "commonjs",
    "files": [
        "./dist/templates",
        "./dist/types",
        "./dist/index.*"
    ],
    "sideEffects": false,
    "keywords": [
        "solana",
        "framework",
        "standard",
        "renderers",
        "Yellowstone",
        "vixen",
        "parser"
    ],
    "scripts": {
        "build": "rimraf dist && tsup && tsc -p ./tsconfig.declarations.json",
        "dev": "vitest --project node",
        "lint": "eslint . && prettier --check .",
        "lint:fix": "eslint --fix . && prettier --write .",
        "publish-package": "pnpm build && changeset publish",
        "test": "vitest run",
        "test:e2e": "./e2e/test.sh"
    },
    "dependencies": {
        "@codama/errors": "^1.3.7",
        "@codama/nodes": "^1.3.7",
        "@codama/renderers-core": "^1.2.2",
        "@codama/visitors-core": "^1.3.7",
        "@codama/renderers-rust": "^1.2.6",
        "@solana/codecs-strings": "^5.0.0",
        "nunjucks": "^3.2.4"
    },
    "devDependencies": {
        "@codama/nodes-from-anchor": "1.2.10",
        "@changesets/changelog-github": "^0.5.1",
        "@changesets/cli": "^2.29.7",
        "@solana/eslint-config-solana": "^5.0.0",
        "@solana/prettier-config-solana": "0.0.5",
        "@types/node": "^24",
        "@types/nunjucks": "^3.2.6",
        "agadoo": "^3.0.0",
        "eslint": "^9.35.0",
        "happy-dom": "^20.0.0",
        "prettier": "^3.6.2",
        "rimraf": "6.1.0",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vitest": "^4.0.1"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/codama-idl/renderers-vixen-parser"
    },
    "bugs": {
        "url": "http://github.com/codama-idl/renderers-vixen-parser/issues"
    },
    "browserslist": [
        "supports bigint and not dead",
        "maintained node versions"
    ],
    "engines": {
        "node": ">=20.18.0"
    },
    "packageManager": "pnpm@10.15.1",
    "prettier": "@solana/prettier-config-solana"
}
